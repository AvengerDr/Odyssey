<Window x:Class="Odyssey.Tools.ShaderGenerator.View.CustomDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CustomDialog" Height="160" Width="320" ShowInTaskbar="False" Topmost="True" ResizeMode="NoResize" WindowStyle="ToolWindow" Loaded="Window_Loaded">
    <StackPanel Background="#FF181818">
        <TextBlock x:Name="title" x:FieldModifier="private" Text="Title" FontSize="16" Foreground="Silver" Margin="8,8,8,0"/>
        <TextBox x:Name="response" x:FieldModifier="private"  Margin="8,8,8,8" TabIndex="0"/>
        <Button Content="Ok" Margin="192 8 8 0" IsDefault="True" Click="Button_Click" />
    </StackPanel>
</Window>
